<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	</ui:style>
	<g:HTMLPanel>
		<g:HorizontalPanel>
			<g:Image ui:field='logo' />
			<g:TextBox ui:field='searchBox'      value='' width="600px" height="20px" />
			<g:Button  ui:field='searchButton'   text='Search' height="26px"/>
			<g:SimplePanel width="25px" />
			<g:Grid>
				<g:row>
					<g:customCell>
						<g:Label text="Total hits: " />
					</g:customCell>
					<g:customCell>
						<g:Label ui:field="totalHits" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="Pages: " />
					</g:customCell>
					<g:customCell>
						<g:HorizontalPanel ui:field="pages" />
					</g:customCell>
				</g:row>
			</g:Grid>
		</g:HorizontalPanel>
	</g:HTMLPanel>

</ui:UiBinder> 