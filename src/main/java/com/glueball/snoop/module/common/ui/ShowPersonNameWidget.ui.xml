<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="editableWidget.css">	
	</ui:style>
	<g:HTMLPanel>
		<g:HorizontalPanel>
			<g:HorizontalPanel>
				<g:Label ui:field="firstName" styleName="{style.fontStyleClientData}" />
				<g:Label text=", " styleName="{style.fontStyleClientData}" />
				<g:Label ui:field="lastName" styleName="{style.fontStyleClientData}" />
				<g:Label text=" " styleName="{style.fontStyleClientData}" />
				<g:Label ui:field="title" styleName="{style.fontStyleClientData}" />
			</g:HorizontalPanel>
		</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 