<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="crmpanel.css">
	</ui:style>
	<g:HTMLPanel>
		<g:SimplePanel styleName='${style.panelStyle}'>
			<g:HorizontalPanel>
				<g:VerticalPanel>
					<g:ScrollPanel ui:field='contentPanel' />
				</g:VerticalPanel>
				<g:HorizontalPanel ui:field='controllerPanel' />
			</g:HorizontalPanel>
		</g:SimplePanel>
	</g:HTMLPanel>
</ui:UiBinder> 