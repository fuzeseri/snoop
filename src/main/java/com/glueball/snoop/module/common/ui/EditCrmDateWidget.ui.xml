<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:dp="urn:import:com.google.gwt.user.datepicker.client" >

	<ui:style src="editableWidget.css">	
	</ui:style>
	<g:HTMLPanel>
		<g:HorizontalPanel>
			<g:Grid>
				<g:row>
					<g:customCell>
						<g:Label text="Year: " styleName="{style.fontStyleClientData}" />
					</g:customCell>
					<g:customCell>
						<g:ListBox ui:field="year" styleName="{style.fontStyleClientData}" />
					</g:customCell>
					<g:customCell>
						<dp:DatePicker ui:field="datePicker" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="Month: " styleName="{style.fontStyleClientData}" />
					</g:customCell>
					<g:customCell>
						<g:ListBox ui:field="month" styleName="{style.fontStyleClientData}" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="Day: " styleName="{style.fontStyleClientData}" />
					</g:customCell>
					<g:customCell>
						<g:ListBox ui:field="day" styleName="{style.fontStyleClientData}" />
					</g:customCell>
				</g:row>
			</g:Grid>
		</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder>