<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="editableWidget.css">	
	</ui:style>
	<g:HTMLPanel>
		<g:HorizontalPanel>
			<g:Grid>
				<g:row>
					<g:customCell>
						<g:Label text="First name: " styleName="{style.fontStyleClientData}" />
					</g:customCell>
					<g:customCell>
						<g:TextBox ui:field="firstName" styleName="{style.fontStyleClientData}" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="Last name: " styleName="{style.fontStyleClientData}" />
					</g:customCell>
					<g:customCell>
						<g:TextBox ui:field="lastName" styleName="{style.fontStyleClientData}" />
					</g:customCell>
				</g:row>
				<g:row>
					<g:customCell>
						<g:Label text="Title: " styleName="{style.fontStyleClientData}" />
					</g:customCell>
					<g:customCell>
						<g:TextBox ui:field="title" styleName="{style.fontStyleClientData}" />
					</g:customCell>
				</g:row>
			</g:Grid>
		</g:HorizontalPanel>
	</g:HTMLPanel>
</ui:UiBinder>