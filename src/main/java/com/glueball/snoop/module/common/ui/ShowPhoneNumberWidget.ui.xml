<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="editableWidget.css">	
	</ui:style>
	<g:HTMLPanel>
		<g:Grid>
			<g:row>
				<g:customCell>
					<g:HorizontalPanel>
						<g:Label text='+' styleName="{style.fontStylePhoneNumber}" />
						<g:Label ui:field='countryCode' styleName="{style.fontStylePhoneNumber}" />
					</g:HorizontalPanel>
				</g:customCell>
				<g:customCell>
					<g:Label ui:field='areaCode' styleName="{style.fontStylePhoneNumber}" />
				</g:customCell>
				<g:customCell>
					<g:Label ui:field='number' styleName="{style.fontStylePhoneNumber}" />
				</g:customCell>
			</g:row>
			<g:row>
				<g:customCell>
					<g:HorizontalPanel>
						<g:Label ui:field='fromHour' styleName="{style.fontStyleValue}" />
						<g:Label text=':' />
						<g:Label ui:field='fromMin' styleName="{style.fontStyleValue}" />
					</g:HorizontalPanel>
				</g:customCell>
				<g:customCell>
					<g:Label text="-" />
				</g:customCell>
				<g:customCell>
					<g:HorizontalPanel>
						<g:Label ui:field='toHour' styleName="{style.fontStyleValue}" />
						<g:Label text=':' styleName="{style.fontStyleValue}" />
						<g:Label ui:field='toMin' styleName="{style.fontStyleValue}" />
					</g:HorizontalPanel>
				</g:customCell>
				<g:customCell>
					<g:Label ui:field='language' styleName="{style.fontStyleValue}" />
				</g:customCell>	
				<g:customCell>
					<g:Label ui:field='sms' styleName="{style.fontStyleValue}" />
				</g:customCell>
			</g:row>
		</g:Grid>
	</g:HTMLPanel>
</ui:UiBinder> 