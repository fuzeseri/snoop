<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="editableWidget.css">	
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel>
			<g:HorizontalPanel>
				<g:VerticalPanel>
					<g:Grid>
						<g:row>
							<g:customCell><g:Label text="Account id:" /></g:customCell>
							<g:customCell><g:Label ui:field="accountId" /></g:customCell>
						</g:row>
						<g:row>
							<g:customCell><g:Label text="IBAN:" /></g:customCell>
							<g:customCell><g:Label ui:field="iban" /></g:customCell>						
						</g:row>
						<g:row>
							<g:customCell><g:Label text="Country code:" /></g:customCell>
							<g:customCell><g:Label ui:field="countryCode" /></g:customCell>
						</g:row>
						<g:row>						
							<g:customCell><g:Label text="Swift Code:" /></g:customCell>
							<g:customCell><g:Label ui:field="swiftCode" /></g:customCell>
						</g:row>
						<g:row>						
							<g:customCell><g:Label text="Bank Name:" /></g:customCell>
							<g:customCell><g:Label ui:field="bankName" /></g:customCell>
						</g:row>
					</g:Grid>
				</g:VerticalPanel>
			</g:HorizontalPanel>
			<g:HorizontalPanel>
				<g:Label ui:field="active" />
				<g:Label text="&nbsp;" />
				<g:Label ui:field="securityDeposit" />
				<g:Label text="&nbsp;" />
				<g:Label ui:field="outgoingTransfer" />
			</g:HorizontalPanel>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder>