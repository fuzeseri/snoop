<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style src="editableWidget.css">	
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel>
			<g:HorizontalPanel>
				<g:Grid>
					<g:row>
						<g:customCell><g:Label text="Account id:" /></g:customCell>
						<g:customCell><g:TextBox ui:field="accountId" /></g:customCell>
					</g:row>
					<g:row>
						<g:customCell><g:Label text="IBAN:" /></g:customCell>
						<g:customCell><g:TextBox ui:field="iban" /></g:customCell>						
					</g:row>
					<g:row>
						<g:customCell><g:Label text="Country code:" /></g:customCell>
						<g:customCell><g:TextBox ui:field="countryCode" /></g:customCell>
					</g:row>
					<g:row>						
						<g:customCell><g:Label text="Swift Code:" /></g:customCell>
						<g:customCell><g:TextBox ui:field="swiftCode" /></g:customCell>
					</g:row>
					<g:row>						
						<g:customCell><g:Label text="Bank Name:" /></g:customCell>
						<g:customCell><g:TextBox ui:field="bankName" /></g:customCell>
					</g:row>
				</g:Grid>
			</g:HorizontalPanel>
			<g:HorizontalPanel>
				<g:Label text="Active " /><g:CheckBox ui:field="active" />
				<g:Label text="Security deposit " /><g:CheckBox ui:field="securityDeposit" />
				<g:Label text="Outgoing Transfer " /><g:CheckBox ui:field="outgoingTransfer" />
			</g:HorizontalPanel>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder>